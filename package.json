{
  "name": "si-ems-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "cd apps/web && npm run dev",
    "dev:mobile": "cd apps/mobile && npm run start",
    "install:all": "npm install && npm install --workspace=apps/web && npm install --workspace=packages/shared"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
